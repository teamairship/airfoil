{
  "extends": ["tslint-config-standard", "tslint-config-prettier"],
  "rules": {
    "strict-type-predicates": false,
    "no-floating-promises": true,
    "curly": [true, "ignore-same-line"]
  },
  "env": {
    "jest": true
  },
  "linterOptions": {
    "exclude": ["**/__TEST__/__MOCKS__/*"]
  }
}
